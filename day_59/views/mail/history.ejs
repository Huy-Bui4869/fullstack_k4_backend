<h2>Lịch sử</h2>
<table class="table table-bordered">
  <thead>
    <tr>
      <th width="5%">STT</th>
      <th>Email gửi đến</th>
      <th>Tiêu đề</th>
      <th>Trạng thái</th>
      <th>Thời gian gửi</th>
      <th>Chi tiết</th>
    </tr>
  </thead>
  <tbody>
    <% value.forEach((el, i) => { %>
    <tr>
      <td><%= i+1 %></td>
      <td><%= el.to %></td>
      <td><%= el.title %></td>
      <td>
        <%- el.status ? `<span class="badge bg-success">Đã xem</span>` : `<span
          class="badge bg-warning"
          >Đã gửi</span
        >` %>
      </td>
      <td><%= getDates(el.created_at) %></td>

      <td>
        <form action="/detail/<%= el.id %>" method="post">
          <button class="btn btn-danger">Xem chi tiết</button>
        </form>
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>
<div class="d-flex justify-content-end">
  <a href="/" class="btn btn-primary">Trở lại</a>
</div>
